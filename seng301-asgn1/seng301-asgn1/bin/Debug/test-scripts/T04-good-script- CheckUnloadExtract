CREATE(5, 10, 25, 100, 255; 3)
CONFIGURE([0] "Coke", 250; "water", 250; "stuff", 205)
COIN_LOAD([0] 0; 10, 1)
POP_LOAD([0] 1; "Coke", 1)
INSERT([0] 123)
EXTRACT([0])
CHECK_DELIVERY(123)
INSERT([0] 255)
PRESS([0] 1)
EXTRACT([0])
CHECK_DELIVERY(10, "Coke")
EXTRACT([0])
CHECK_DELIVERY(0)
UNLOAD([0])
CHECK_TEARDOWN(0; 255)
UNLOAD([0])
CHECK_TEARDOWN(0; 0)